  gem 'jekyll-paginate'
  gem 'jekyll-tagsgenerator'
  gem 'jekyll-seo-tag'
  gem 'jekyll-sitemap'
  gem 'jekyll-archives'
  gem 'jekyll-webmention_io'
  gem 'jekyll-feed'
  gem 'jekyll-cloudinary'
end

